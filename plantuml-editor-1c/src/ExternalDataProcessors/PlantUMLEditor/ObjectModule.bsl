Функция ТипыСтрелок()

	ТипыСтрелокСвязи = Новый СписокЗначений();
	ТипыСтрелокСвязи.Добавить("", НСтр("ru = 'Авто'"));
	ТипыСтрелокСвязи.Добавить("->", "->");
	ТипыСтрелокСвязи.Добавить("->>", "->>");
	ТипыСтрелокСвязи.Добавить("-\", "-\");
	ТипыСтрелокСвязи.Добавить("-\\", "-\\");
	ТипыСтрелокСвязи.Добавить("-/", "-/");
	ТипыСтрелокСвязи.Добавить("-//", "-//");
	ТипыСтрелокСвязи.Добавить("->x", "->x");
	ТипыСтрелокСвязи.Добавить("x->", "x->");
