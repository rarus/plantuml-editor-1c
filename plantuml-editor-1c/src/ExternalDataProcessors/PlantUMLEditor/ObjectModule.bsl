	ДобавитьСвойство(
		Свойства,
		ТипДиаграммы,
		"skinparam sequenceMessageAlign",
		НСтр("ru = 'Расположение текста относительно стрелки'"),
		Новый ОписаниеТипов("Строка"),
		СписокВыбораРасположения(),
